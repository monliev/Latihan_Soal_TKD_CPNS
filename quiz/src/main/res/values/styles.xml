<?xml version="1.0" encoding="utf-8"?>

<!-- Generated with http://android-holo-colors.com -->
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="EditTextstyled" parent="android:Widget.EditText">
        <item name="android:background">@drawable/styled_edit_text_holo_light</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="CheckBoxstyled" parent="android:Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/styled_btn_check_holo_light</item>
    </style>

    <style name="RadioButtonstyled" parent="android:Widget.CompoundButton.RadioButton">
        <item name="android:button">@drawable/styled_btn_radio_holo_light</item>
    </style>

    <style name="Buttonstyled" parent="android:Widget.Button">
        <item name="android:background">@drawable/styled_btn_default_holo_light</item>
        <item name="android:minHeight">48dip</item>
        <item name="android:minWidth">64dip</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="ImageButtonstyled" parent="android:Widget.ImageButton">
        <item name="android:background">@drawable/styled_btn_default_holo_light</item>
    </style>

    <style name="Spinnerstyled" parent="android:Widget.Spinner">
        <item name="android:background">@drawable/styled_spinner_background_holo_light</item>
        <item name="android:dropDownSelector">@drawable/styled_list_selector_holo_light</item>
    </style>

    <style name="SpinnerDropDownItemstyled" parent="android:Widget.DropDownItem.Spinner">
        <item name="android:checkMark">@drawable/styled_btn_radio_holo_light</item>
    </style>


    <!-- ActionBar styles -->
    <style name="MyActionBar" parent="@style/Widget.AppCompat.ActionBar">
        <item name="android:titleTextStyle">@style/MyActionBarTitleText</item>
        <item name="background">@drawable/ab_transparent_styled</item>

        <!-- Support library compatibility -->
        <item name="titleTextStyle">@style/MyActionBarTitleText</item>
    </style>

    <!-- ActionBar title text -->
    <style name="MyActionBarTitleText" parent="@style/TextAppearance.AppCompat.Widget.ActionBar.Title">
        <item name="android:textColor">@color/actionbar_text</item>
        <!-- The textColor property is backward compatible with the Support Library -->
    </style>


</resources>